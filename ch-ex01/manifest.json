{
  "manifest_version": 3,
  "name": "yt-thumbnail-player",
  "description": "you love thumbnails.",
  "version": "1.0",
  "icons": {
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "player.js"
      ],
      "matches": [
        "https://www.youtube.com*"
      ]
    }
  ]
}
